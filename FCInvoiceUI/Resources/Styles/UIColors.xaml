<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- Background Colors -->
    <Color x:Key="primaryBackColor">#f1f2f6</Color>
    <Color x:Key="secondaryBackColor">#ced6e0</Color>
    
    <!-- Panel Colors -->
    <Color x:Key="panelColor">#70a1ff</Color>
    
    <!-- Font Color -->
    <SolidColorBrush x:Key="textColor" Color="#2f3542"/>

</ResourceDictionary>